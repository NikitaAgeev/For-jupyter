   "source": [
    "import numpy as np\n",
    "import scipy\n",
    "\n",
    "N =16\n",
    "n = 0\n",
    "\n",
    "def F(n):\n",
    "    res = 0\n",
    "    for i in range(N):\n",
    "        res += np.cos(2*np.pi*5*i/16)*np.exp(-1j*2*np.pi/N*n*i)\n",
    "    return res;\n",
    "\n",
    "for i in range(N):\n",
    "    print(i, \" | \", F(i))"
   ]
